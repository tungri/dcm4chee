<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server PUBLIC
    "-//JBoss//DTD MBean Service 4.2//EN"
    "http://www.jboss.org/j2ee/dtd/jboss-service_4_2.dtd">

<server>
  <classpath codebase="lib" archives="dcm4chee.jar"/>
